    public int sumOfEvaluation()
    {
        int sum = 0;
        if(evaluations != null && !evaluations.isEmpty())
        {
            for(Evaluation eval : evaluations)
            {
                sum = sum + eval.getExpertAssessment1()+eval.getExpertAssessment2()+eval.getExpertAssessment3()+eval.getExpertAssessment4()+eval.getExpertAssessment5();
            }
        }
        return sum;
    }



}

/*--- ВЫПАДАЮЩИЕ ПУНКТЫ ---*/
.site-header .nav-menu ul ul{
    list-style:none;
    position:absolute;
    left:-9999px; /* Скрываем за экраном, когда не нужно (данный метод лучше, чем display:none;) */
    opacity:0; /* Устнавливаем начальное состояние прозрачности */
    -webkit-transition:0.25s linear opacity; /* В Webkit выпадающие пункты будут проявляться */
}
.site-header .nav-menu ul ul li{
    padding-top:1px; /* Вводим отступ между li чтобы создать иллюзию разделенных пунктов меню */
    float:none;
}

.site-header .nav-menu ul ul li a,
.site-header .nav-menu .current_page_item ul a{
    display:block;
    width: 100px;
    background:#eee;
    color: #0a7096;
    line-height: 1.2;
    padding: 8px;
}

.site-header .nav-menu ul li:hover ul{ /* Выводим выпадающий пункт при наведении курсора */
    left:0; /* Приносим его обратно на экран, когда нужно */
    opacity:1; /* Делаем непрозрачным */
}

.site-header .nav-menu ul li:hover ul li a:hover{ /* Устанавливаем стили для выпадающих пунктов, когда курсор наводится на конкретный пункт */
    background:#ddd;
    color: #0a7096;
    -moz-transform:scale(1.05);
    -webkit-transform:scale(1.05);
}

